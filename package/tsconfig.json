{
	"compileOnSave": false,
	"compilerOptions": {
		"lib": [
			"es5",
			"es6",
			"es7",
			"esnext"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"removeComments": false,
		"preserveConstEnums": true,
		"target": "esnext",
		"declaration": true,
		"emitDecoratorMetadata": true,
		"sourceMap": true,
		"experimentalDecorators": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noImplicitThis": true,
		"noEmitOnError": true,
		"noEmit": false,
		"alwaysStrict": true,
		"typeRoots": [],
		"types": []
	}
}